<h1 class="text-center">TO-DO</h1>

<div class="row">
    <div class="col-md-6">
        <h4>Songs</h4><br>
        {{#each song}}
            <p>
                <button role="button" class="btn btn-primary btn-sm song-btn" data-id="{{this.id}}" data-tablename="{{this.table}}" data-tasks="{{this.tasks}}">
                    <span class="font-italic">{{this.title}}</span>
                </button>
            </p>
        {{/each}}
    </div>

    <div class="col-md-6" id="taskView">
        <h4>To-dos</h4><br>

    </div>
</div>